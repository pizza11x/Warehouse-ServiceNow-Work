        <name>Switch Assignment group readonly</name>
        <order/>
        <script><![CDATA[function onLoad() {
	if(g_form.getValue('short_description')=='This is a task for check the order')
	{
		var ga = new GlideAjax('CheckMyGroup');
		ga.addParam('sysparm_name', 'myGroup');
		ga.getXML(callBack);
	}
	else
		g_form.setVisible('assignment_group', false);
	
	function callBack(response) {
        var answer = response.responseXML.documentElement.getAttribute("answer");
		if(answer==='false')
		{
			g_form.setVisible('assignment_group', false);
		}
	}
}]]></script>
