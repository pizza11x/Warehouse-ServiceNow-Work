        <name>Move To Warehouse</name>
        <onclick>moveToWarehouse()</onclick>
        <order>100</order>
        <script><![CDATA[function moveToWarehouse()
{
	var recIds = g_list.getChecked();
	var recIdsList = recIds.split(',');
	if(recIdsList.length > 1)
	{
		alert('Select only one record to move');
		return;
	}
	else{
		var qToMove = 0;
		var ga = new GlideAjax('MoveToWhScript');
		var wh = prompt('Enter the warehouse: ', 'warehouse');
		qToMove += prompt('Enter the quantity to move: ', 'quantity');
		parseInt(qToMove);
		ga.addParam('sysparm_name', 'wareUpdate');
		ga.addParam('sysparm_id', recIdsList[0]);
		ga.addParam('sysparm_wh', wh);
		ga.addParam('sysparm_qToMove', qToMove);
		ga.getXML(callBack);
		
	}
}
	
	function callBack(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");
		if(answer){
			setTimeout(function(){
				this.window.location.reload();
			}, 300);
		}
		
	}
	
	
]]></script>
